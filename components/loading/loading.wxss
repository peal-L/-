.loading {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  justify-content: center;
  align-items: center;
  display: none;
}

.show {
  display: flex;
}

.loading_content {
  background-color: #ccc;
  padding: 40px;
  border-radius: 12px;
}

.text {
  color: #fff;
  font-size: 14px;
  text-align: center;
}

.svg {
  width: 120px;
  height: 120px;
  background-image: url("data:image/svg+xml;base64,");
}